<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Certify</servlet-name>
        <servlet-class>Certify</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Certify</servlet-name>
        <url-pattern>/certify</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SavePhoto</servlet-name>
        <servlet-class>SavePhoto</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SavePhoto</servlet-name>
        <url-pattern>/savePhoto</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Transaction</servlet-name>
        <servlet-class>Transaction</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Transaction</servlet-name>
        <url-pattern>/transaction</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetTran</servlet-name>
        <servlet-class>GetTran</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetTran</servlet-name>
        <url-pattern>/getTran</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>GetPhoto</servlet-name>
        <servlet-class>GetPhoto</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetPhoto</servlet-name>
        <url-pattern>/getPhoto</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>Search</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
</web-app>